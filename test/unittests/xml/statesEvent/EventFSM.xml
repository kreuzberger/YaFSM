<root>
<definitions>
  <action name="onEventAutoEnd" param=""/>
  <action name="onEventAutoStart" param=""/>
  <action name="onSendAutoStart" param=""/>
  <action name="onEnterRunning" param=""/>
  <trigger name="sendAutoStart" param=""/>
  <event name="AutoStart"/>
  <event name="AutoEnd"/>
</definitions>
<fsm>
  <state name="stop" type="entry"/>
  <state name="run">
    <state name="running" type="entry" enter="onEnterRunning()"/>
  </state>
  <state name="finished"/>
  <transition trigger="eventAutoStart" action="onEventAutoStart()" begin="stop" end="run" condition=""/>
  <transition trigger="eventAutoEnd" action="onEventAutoEnd()" begin="run" end="finished"/>
  <transition trigger="sendAutoStart" action="onSendAutoStart()" event="AutoStart" begin="stop" end="stop"/>
</fsm>
</root>
